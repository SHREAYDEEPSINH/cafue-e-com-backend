{
  "version": 2,
  "builds": [{ "src": "./index.js", "use": "@vercel/node" }],
  "functions": {
    "index.js": { "maxDuration": 30 } 
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
